الطيران المحلي = صحيح
deb المحلي = صحيح
السيطرة المحلية = {f = 0، b = 0، l = 0، r = 0}
local lastctrl = {f = 0, b = 0, l = 0, r = 0}
دالة KeyDownFunction المحلية = لا شيء
دالة KeyUpFunction المحلية = لا شيء
Fly_Button.MouseButton1Click:Connect(function()
	تغيير لون التبديل (زر الطيران)
	إذا كان Fly_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) إذن
		الطيران = صحيح
		إذا كانت اللعبة: GetService("UserInputService").TouchEnabled، فعندئذٍ
			Fly_Pad.Visible = صحيح
		نهاية
		الجذع العلوي المحلي = plr.Character.UpperTorso
		السرعة المحلية = 0
		الوظيفة المحلية Fly()
			bg محلي = Instance.new("BodyGyro", UpperTorso)
			bg.P = 9e4
			bg.maxTorque = Vector3.new(9e9, 9e9, 9e9)
			bg.cframe = UpperTorso.CFrame
			محلي bv = Instance.new("BodyVelocity", UpperTorso)
			bv.velocity = Vector3.new(0,0.1,0)
			bv.maxForce = Vector3.new(9e9, 9e9, 9e9)
			بلاي أنيم(10714347256,4,0)
			كرر المهمة.انتظر
				plr.Character.Humanoid.PlatformStand = صحيح
				إذا كان ctrl.l + ​​ctrl.r ~= 0 أو ctrl.f + ctrl.b ~= 0، فإن
					السرعة = السرعة + سرعة الطيران * 0.10
					إذا كانت السرعة > FlySpeed ​​إذن
						السرعة = FlySpeed
					نهاية
				وإلا إذا لم يكن (ctrl.l + ​​ctrl.r ~= 0 أو ctrl.f + ctrl.b ~= 0) والسرعة ~= 0 إذن
					السرعة = السرعة-سرعة الطيران*0.10
					إذا كانت السرعة < 0 فإن
						السرعة = 0
					نهاية
				نهاية
				إذا كان (ctrl.l + ​​ctrl.r) ~= 0 أو (ctrl.f + ctrl.b) ~= 0 إذن
					bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (ctrl.f+ctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(ctrl.l+ctrl.r,(ctrl.f+ctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed
					lastctrl = {f = ctrl.f، b = ctrl.b، l = ctrl.l، r = ctrl.r}
				وإلا إذا كان (ctrl.l + ​​ctrl.r) == 0 و (ctrl.f + ctrl.b) == 0 والسرعة ~= 0 إذن
					bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (lastctrl.f+lastctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(lastctrl.l+lastctrl.r,(lastctrl.f+lastctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed
				آخر
					bv.velocity = Vector3.new(0,0.1,0)
				نهاية
				bg.cframe = game.Workspace.CurrentCamera.CoordinateFrame * CFrame.Angles(-math.rad((ctrl.f+ctrl.b)*50*speed/FlySpeed),0,0)
			حتى لا تطير
			ctrl = {f = 0, b = 0, l = 0, r = 0}
			lastctrl = {f = 0, b = 0, l = 0, r = 0}
			السرعة = 0
			bg:تدمير()
			bv:تدمير()
			plr.Character.Humanoid.PlatformStand = false
		نهاية

		دالة KeyDown = الماوس.KeyDown: الاتصال (دالة (المفتاح)
			إذا كان key:lower() == "w" إذن
				ctrl.f = 1
				PlayAnim(10714177846,4.65,0)
			elseif key:lower() == "s" ثم
				ctrl.b = -1
				PlayAnim(10147823318,4.11,0)
			elseif key:lower() == "a" ثم
				ctrl.l = -1
				PlayAnim(10147823318,3.55,0)
			elseif key:lower() == "d" ثم
				ctrl.r = 1
				PlayAnim(10147823318,4.81,0)
			نهاية
		نهاية)

		KeyUpFunction = الماوس.KeyUp: الاتصال (وظيفة (المفتاح)
			إذا كان key:lower() == "w" إذن
				ctrl.f = 0
				بلاي أنيم(10714347256,4,0)
			elseif key:lower() == "s" ثم
				ctrl.b = 0
				بلاي أنيم(10714347256,4,0)
			elseif key:lower() == "a" ثم
				ctrl.l = 0
				بلاي أنيم(10714347256,4,0)
			elseif key:lower() == "d" ثم
				ctrl.r = 0
				بلاي أنيم(10714347256,4,0)
			نهاية
		نهاية)
		سافر()
	آخر
		الطيران = خطأ
		Fly_Pad.Visible = false
		دالة KeyDown:Disconnect()
		KeyUpFunction:Disconnect()
		إيقاف الرسوم المتحركة
	نهاية
نهاية)

