	إذا كان httprequest إذن
		الخوادم المحلية = {}
		الطلب المحلي = httprequest({عنوان URL = string.format("https://games.roblox.com/v1/games/%d/servers/Public?sortOrder=Desc&limit=100", game.PlaceId)})
		الجسم المحلي = HttpService:JSONDecode(req.Body)
		إذا كان الجسم وبيانات الجسم إذن
			بالنسبة إلى i، v في التالي، body.data
				إذا كان type(v) == "table" و tonumber(v.playing) و tonumber(v.maxPlayers) و v.playing < v.maxPlayers و v.id ~= game.JobId، إذن
					الجدول.إدراج(الخوادم، 1، معرف v.)
				نهاية
			نهاية
		نهاية
		إذا كان #servers > 0 إذن
			اللعبة: GetService("TeleportService"): TeleportToPlaceInstance(game.PlaceId, servers[math.random(1, #servers)], plr)
		نهاية
	نهاية
نهاية)

