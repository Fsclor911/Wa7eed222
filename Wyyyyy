--إعادة تحميل واجهة المستخدم الرسومية
إذا كانت اللعبة.CoreGui:FindFirstChild("Wa7eed") إذن
	اللعبة: GetService("StarterGui"): SetCore("SendNotification", {Title = "~Wa7eed~",Text = "تم تحميل واجهة المستخدم الرسومية بالفعل، أعد الانضمام لإعادة التنفيذ",Duration = 5;})
	يعود
نهاية
الإصدار المحلي = 2
--المتغيرات
_G.AntiFlingToggled = false
اللاعبون المحليون = اللعبة: GetService("اللاعبون")
plr المحلي = Players.LocalPlayer
خدمة Tween المحلية = اللعبة: GetService("TweenService")
RunService المحلي = اللعبة:GetService("RunService")
الضوء المحلي = اللعبة: GetService("الإضاءة")
خدمة HttpService المحلية = game:GetService("HttpService")
طلب http المحلي = (syn و syn.request) أو (http و http.request) أو http_request أو (fluxus و fluxus.request) أو request
الفأرة المحلية = plr:GetMouse()
القائمة البيضاء للنصوص المحلية = {}
قائمة ForceWhitelist المحلية = {}
لاعب مستهدف محلي = لا شيء
سرعة الطيران المحلية = 50
أداة الجرعة المحلية = لا شيء
نقطة التحقق المحفوظة المحلية = لا شيء
مجلد المناجم المحلي = لا شيء
FreeEmotesEnabled المحلي = false
مجلدات CannonsFolders المحلية = {}

استدعاء (دالة)
	مجلد الألغام = اللعبة: GetService("مساحة العمل").الألغام الأرضية
	بالنسبة إلى i وv في أزواج (اللعبة: GetService("مساحة العمل"): GetChildren()) افعل
		إذا كان v.Name == "المدافع" إذن
			table.insert(CannonsFolders, v)
		نهاية
	نهاية
نهاية)
--الوظائف
_G.shield = function(id)
	إذا لم يكن table.find(ForceWhitelist,id) إذن
		table.insert(ForceWhitelist، id)
	نهاية
نهاية

الوظيفة المحلية RandomChar()
	الطول المحلي = math.random(1,5)
	المصفوفة المحلية = {}
	بالنسبة إلى i = 1، الطول do
		مصفوفة[i] = سلسلة.char(math.random(32, 126))
	نهاية
	إرجاع table.concat(array)
نهاية

الوظيفة المحلية ChangeToggleColor(Button)
	led = زر.Ticket_Asset
	إذا led.ImageColor3 == Color3.fromRGB(255, 255, 255) فإن -- أبيض
		led.ImageColor3 = Color3.fromRGB(100, 100, 100) -- رمادي
	آخر
		led.ImageColor3 = Color3.fromRGB(255, 255, 255) -- أبيض
	نهاية
نهاية

الوظيفة المحلية GetPing()
	العودة (اللعبة: GetService("Stats").Network.ServerStatsItem["بيانات Ping"]: GetValue())/1000
نهاية

الوظيفة المحلية GetPlayer(UserDisplay)
	إذا كان UserDisplay ~= "" إذن
        بالنسبة إلى i،v في أزواج (Players:GetPlayers()) افعل
            إذا كان v.Name:lower():match(UserDisplay) أو v.DisplayName:lower():match(UserDisplay) إذن
                العودة v
            نهاية
        نهاية
		العودة صفر
	آخر
		العودة صفر
    نهاية
نهاية

