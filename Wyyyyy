AntiFling_Button.MouseButton1Click:Connect(function()
	تغيير لون التبديل (زر منع القذف)
	إذا كان AntiFling_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) إذن
		_G.AntiFlingToggled = صحيح
		سلسلة تحميل(اللعبة:HttpGet('https://raw.githubusercontent.com/H20CalibreYT/SystemBroken/main/AntiFling'))()
	آخر
		_G.AntiFlingToggled = false
	نهاية
نهاية)

AntiChatSpy_Button.MouseButton1Click:Connect(function()
	تغيير لون التبديل (زر مكافحة التجسس على الدردشة)
	إذا كان AntiChatSpy_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) إذن
		كرر المهمة.انتظر
			اللاعبون: الدردشة (RandomChar())
		حتى AntiChatSpy_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(255,0,0)
	نهاية
نهاية)

دالة AntiAFK المحلية = لا شيء
AntiAFK_Button.MouseButton1Click:Connect(function()
	تغيير لون التبديل (زر AntiAFK)
	إذا كان AntiAFK_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) إذن
		دالة AntiAFK = plr.Idled:Connect(دالة())
			المستخدم الافتراضي المحلي = اللعبة: GetService("VirtualUser")
			المستخدم الافتراضي:CaptureController()
			المستخدم الافتراضي:ClickButton2(Vector2.new())
		نهاية)
	آخر
		وظيفة AntiAFK:Disconnect()
	نهاية
نهاية)

Shaders_Button.MouseButton1Click:Connect(function()
	تغيير لون التبديل (زر التظليل)
	إذا كان Shaders_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0,255,0) إذن
		السماء المحلية = Instance.new("السماء")
		محلي Bloom = Instance.new("BloomEffect")
		محلي Blur = Instance.new("BlurEffec t")
		ColorC المحلي = Instance.new("ColorCorrectionEffect")
		أشعة الشمس المحلية = Instance.new("SunRaysEffect")

		سطوع الضوء = 2.25
		تعويض التعرض للضوء = 0.1
		وقت الساعة الخفيفة = 17.55

		Sky.SkyboxBk = "http://www.roblox.com/asset/?id=144933338"
		Sky.SkyboxDn = "http://www.roblox.com/asset/?id=144931530"
		Sky.SkyboxFt = "http://www.roblox.com/asset/?id=144933262"
		Sky.SkyboxLf = "http://www.roblox.com/asset/?id=144933244"
		Sky.SkyboxRt = "http://www.roblox.com/asset/?id=144933299"
		Sky.SkyboxUp = "http://www.roblox.com/asset/?id=144931564"
		عدد نجوم السماء = 5000
		Sky.SunAngularSize = 5
		Sky.Parent = الضوء

		كثافة الإزهار = 0.3
		بلوم.الحجم = 10
		عتبة بلوم = 0.8
		بلوم.بارنت = الضوء

		حجم التمويه = 5
		Blur.Parent = ضوء

		ColorC.Brightness = 0
		ColorC.Contrast = 0.1
		ColorC.Saturation = 0.25
		ColorC.TintColor = Color3.fromRGB(255، 255، 255)
		ColorC.Parent = فاتح

		أشعة الشمس.الكثافة = 0.1
		انتشار أشعة الشمس = 0.8
		SunRays.Parent = الضوء
	آخر
		بالنسبة إلى i،v في أزواج (Light:GetChildren()) افعل
			v:تدمير()
		نهاية
		الضوء والسطوع = 2
		تعويض التعرض للضوء = 0
	نهاية
نهاية)

