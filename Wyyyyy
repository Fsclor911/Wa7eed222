الوظيفة المحلية TeleportTO(posX,posY,posZ,player,method)
	استدعاء (دالة)
		إذا كانت الطريقة == "آمنة" إذن
			task.spawn(دالة())
				لأن i = 1,30
					مهمة.انتظر()
					الحصول على الجذر (plr).السرعة = Vector3.new(0,0,0)
					إذا كان اللاعب == "pos" إذن
						GetRoot(plr).CFrame = CFrame.new(posX,posY,posZ)
					آخر
						الحصول على الجذر (plr).CFrame = CFrame.new (الحصول على الجذر (اللاعب). الموقع) + Vector3.new (0،2،0)
					نهاية
				نهاية
			نهاية)
		آخر
			الحصول على الجذر (plr).السرعة = Vector3.new(0,0,0)
			إذا كان اللاعب == "pos" إذن
				GetRoot(plr).CFrame = CFrame.new(posX,posY,posZ)
			آخر
				الحصول على الجذر (plr).CFrame = CFrame.new (الحصول على الجذر (اللاعب). الموقع) + Vector3.new (0،2،0)
			نهاية
		نهاية
	نهاية)
نهاية

الوظيفة المحلية PredictionTP(player,method)
	الجذر المحلي = GetRoot(اللاعب)
	الموضع المحلي = الموضع الجذر
	السرعة المحلية = السرعة الجذرية
	احصل على الجذر (plr). CFrame = CFrame. جديد ((pos.X) + (vel.X) * (GetPing() * 3.5) ، (pos.Y) + (vel.Y) * (GetPing() * 2) ، (pos.Z) + (vel.Z) * (GetPing() * 3.5))
	إذا كانت الطريقة == "آمنة" إذن
		مهمة.انتظر()
		GetRoot(plr).CFrame = CFrame.new(pos)
		مهمة.انتظر()
		احصل على الجذر (plr). CFrame = CFrame. جديد ((pos.X) + (vel.X) * (GetPing() * 3.5) ، (pos.Y) + (vel.Y) * (GetPing() * 2) ، (pos.Z) + (vel.Z) * (GetPing() * 3.5))
	نهاية
نهاية

الوظيفة المحلية Touch(x,root)
	استدعاء (دالة)
		x = x:FindFirstAncestorWhichIsA("الجزء")
		إذا x إذن
			إذا كان firetouchinterest إذن
				task.spawn(دالة())
					firetouchinterest(x، الجذر، 1)
					مهمة.انتظر()
					firetouchinterest(x، الجذر، 0)
				نهاية)
			نهاية
		نهاية
	نهاية)
نهاية


الوظيفة المحلية GetPush()
	TempPush المحلي = صفر
	استدعاء (دالة)
		إذا كان plr.Backpack:FindFirstChild("Push") إذن
			أداة الدفع = plr.حقيبة الظهر.ادفع
			PushTool.Parent = plr.Character
			TempPush = أداة الدفع
		نهاية
		بالنسبة إلى i،v في أزواج (Players:GetPlayers()) افعل
			إذا كان v.Character:FindFirstChild("Push") إذن
				TempPush = v.Character.Push
			نهاية
		نهاية
	نهاية)
	العودة TempPush
نهاية

الوظيفة المحلية CheckPotion()
	إذا كان plr.Backpack:FindFirstChild("potion") إذن
		أداة الجرعة = plr.Backpack:FindFirstChild("الجرعة")
		العودة صحيح
	elseif plr.Character:FindFirstChild("potion") then
		أداة الجرعة = plr.Character:FindFirstChild("الجرعة")
		العودة صحيح
	آخر
		العودة خطأ
	نهاية
نهاية

الوظيفة المحلية Push(Target)
	الدفع المحلي = GetPush()
	FixTool المحلي = لا شيء
	إذا كان الدفع ~= لا شيء إذن
		الحجج المحلية = {[1] = Target.Character}
		GetPush().PushTool:FireServer(unpack(args))
	نهاية
	إذا كان plr.Character:FindFirstChild("Push") إذن
		plr.Character.Push.Parent = plr.Backpack
	نهاية
	إذا كان plr.Character:FindFirstChild("ModdedPush") إذن
		FixTool = plr.Character:FindFirstChild("ModdedPush")
		FixTool.Parent = plr.Backpack
		FixTool.Parent = plr.Character
	نهاية
	إذا كان plr.Character:FindFirstChild("ClickTarget") إذن
		FixTool = plr.Character:FindFirstChild("ClickTarget")
		FixTool.Parent = plr.Backpack
		FixTool.Parent = plr.Character
	نهاية
	إذا كان plr.Character:FindFirstChild("potion") إذن
		FixTool = plr.Character:FindFirstChild("potion")
		FixTool.Parent = plr.Backpack
		FixTool.Parent = plr.Character
	نهاية
نهاية



