--SCRIPTS
local function ToggleFly(bool)
	if bool then
		local bodyVelocity = Instance.new("BodyVelocity")
		bodyVelocity.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
		bodyVelocity.Velocity = Vector3.new(0, 0, 0)
		bodyVelocity.Parent = plr.Character.HumanoidRootPart
		local bodyGyro = Instance.new("BodyGyro")
		bodyGyro.MaxTorque = Vector3.new(math.huge, math.huge, math.huge)
		bodyGyro.CFrame = plr.Character.HumanoidRootPart.CFrame
		bodyGyro.Parent = plr.Character.HumanoidRootPart
		local function onRenderStep()
			if Fly_Button.Ticket_Asset.ImageColor3 == Color3.fromRGB(0, 255, 0) then
				local camera = game.Workspace.CurrentCamera
				local moveDirection = Vector3.new(0, 0, 0)
				if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.W) then
					moveDirection = moveDirection + camera.CFrame.lookVector
				end
				if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.S) then
					moveDirection = moveDirection - camera.CFrame.lookVector
				end
				if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.A) then
					moveDirection = moveDirection - camera.CFrame.rightVector
				end
				if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.D) then
					moveDirection = moveDirection + camera.CFrame.rightVector
				end
				bodyVelocity.Velocity = moveDirection.unit * FlySpeed
				bodyGyro.CFrame = camera.CFrame
			else
				bodyVelocity:Destroy()
				bodyGyro:Destroy()
			end
		end
		RunService:BindToRenderStep("Fly", Enum.RenderPriority.Camera.Value + 1, onRenderStep)
	else
		RunService:UnbindFromRenderStep("Fly")
		if plr.Character.HumanoidRootPart:FindFirstChildOfClass("BodyVelocity") then
			plr.Character.HumanoidRootPart:FindFirstChildOfClass("BodyVelocity"):Destroy()
		end
		if plr.Character.HumanoidRootPart:FindFirstChildOfClass("BodyGyro") then
			plr.Character.HumanoidRootPart:FindFirstChildOfClass("BodyGyro"):Destroy()
		end
	end
end

local function AntiFling(bool)
	if bool then
		_G.AntiFlingToggled = true
		local connection
		connection = plr.Character.Humanoid.Touched:Connect(function(hit)
			if _G.AntiFlingToggled and hit.Parent:FindFirstChildOfClass("Humanoid") and hit.Parent.Name ~= plr.Name then
				local char = plr.Character
				local root = char:FindFirstChild("HumanoidRootPart")
				if root then
					root.Velocity = Vector3.new(0, 0, 0)
					root.RotVelocity = Vector3.new(0, 0, 0)
				end
			end
		end)
		AntiFling_Button.Tag = connection
	else
		_G.AntiFlingToggled = false
		if AntiFling_Button.Tag then
			AntiFling_Button.Tag:Disconnect()
			AntiFling_Button.Tag = nil
		end
	end
end

local function AntiChatSpy(bool)
	if bool then
		local oldName = plr.Name
		plr.Name = RandomChar()
		AntiChatSpy_Button.Tag = oldName
	else
		if AntiChatSpy_Button.Tag then
			plr.Name = AntiChatSpy_Button.Tag
			AntiChatSpy_Button.Tag = nil
		end
	end
end

local function AntiAFK(bool)
	if bool then
		local afkConnection = RunService.RenderStepped:Connect(function()
			game:GetService("VirtualUser"):CaptureController()
			game:GetService("VirtualUser"):ClickButton2(Vector2.new(0,0), 0)
		end)
		AntiAFK_Button.Tag = afkConnection
	else
		if AntiAFK_Button.Tag then
			AntiAFK_Button.Tag:Disconnect()
			AntiAFK_Button.Tag = nil
		end
	end
end

local function ChatAlert(bool)
	if bool then
		local chatConnection = game:GetService("Chat").Chatted:Connect(function(msg)
			SendNotify("رسالة جديدة", msg, 5)
		end)
		ChatAlert_Button.Tag = chatConnection
	else
		if ChatAlert_Button.Tag then
			ChatAlert_Button.Tag:Disconnect()
			ChatAlert_Button.Tag = nil
		end
	end
end

local function FreeEmotes(bool)
	if bool then
		FreeEmotesEnabled = true
		local oldEmote = plr.Character.Animate.emote.Value
		plr.Character.Animate.emote.Value = ""
		FreeEmotes_Button.Tag = oldEmote
	else
		FreeEmotesEnabled = false
		if FreeEmotes_Button.Tag then
			plr.Character.Animate.emote.Value = FreeEmotes_Button.Tag
			FreeEmotes_Button.Tag = nil
		end
	end
end

local function Headsit(bool)
	if bool then
		local target = TargetedPlayer
		if target and target.Character and plr.Character then
			local root = plr.Character:FindFirstChild("HumanoidRootPart")
			local targetRoot = target.Character:FindFirstChild("HumanoidRootPart")
			if root and targetRoot then
				local weld = Instance.new("Weld")
				weld.Part0 = root
				weld.Part1 = targetRoot
				weld.C0 = CFrame.new(0, 2, 0)
				weld.Parent = root
				HeadsitTarget_Button.Tag = weld
		end
		end
	else
		if HeadsitTarget_Button.Tag then
			HeadsitTarget_Button.Tag:Destroy()
			HeadsitTarget_Button.Tag = nil
		end
	end
end

local function Drag(bool)
	if bool then
		local target = TargetedPlayer
		if target and target.Character and plr.Character then
			local root = plr.Character:FindFirstChild("HumanoidRootPart")
			local targetRoot = target.Character:FindFirstChild("HumanoidRootPart")
			if root and targetRoot then
				local weld = Instance.new("Weld")
				weld.Part0 = root
				weld.Part1 = targetRoot
				weld.C0 = CFrame.new(0, 0, -5)
				weld.Parent = root
				DragTarget_Button.Tag = weld
		end
		end
	else
		if DragTarget_Button.Tag then
			DragTarget_Button.Tag:Destroy()
			DragTarget_Button.Tag = nil
		end
	end
end

