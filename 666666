ClickTargetTool_Button.MouseButton1Click:Connect(function()
	local tool = Instance.new("Tool")
	tool.Name = "ClickTarget"
	tool.Parent = plr.Backpack
	local part = Instance.new("Part")
	part.Size = Vector3.new(1,1,1)
	part.Transparency = 1
	part.CanCollide = false
	part.Parent = tool
	tool.Activated:Connect(function()
		if mouse.Target and mouse.Target.Parent and mouse.Target.Parent:FindFirstChildOfClass("Humanoid") then
			TargetedPlayer = Players:GetPlayerFromCharacter(mouse.Target.Parent)
			if TargetedPlayer then
				TargetName_Input.Text = TargetedPlayer.DisplayName
				UserIDTargetLabel.Text = "ID: "..TargetedPlayer.UserId
				TargetImage.Image = "rbxthumb://v1/users/"..TargetedPlayer.UserId.."/headshot?format=png&size=48x48"
			end
		end
	end)
end)

ViewTarget_Button.MouseButton1Click:Connect(function()
	if TargetedPlayer and TargetedPlayer.Character and plr.Character then
		local root = plr.Character:FindFirstChild("HumanoidRootPart")
		local targetRoot = TargetedPlayer.Character:FindFirstChild("HumanoidRootPart")
		if root and targetRoot then
			game.Workspace.CurrentCamera.CameraSubject = TargetedPlayer.Character.Humanoid
		end
	end
end)

FlingTarget_Button.MouseButton1Click:Connect(function()
	if TargetedPlayer and TargetedPlayer.Character then
		local root = TargetedPlayer.Character:FindFirstChild("HumanoidRootPart")
		if root then
			root.Velocity = Vector3.new(0, 500, 0)
		end
	end
end)

