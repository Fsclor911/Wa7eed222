Serverhop_Button.MouseButton1Click:Connect(function()
	local servers = game:GetService("HttpService"):JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/"..game.PlaceId.."/servers/Public?limit=100"))
	local currentJobId = game.JobId
	for i,v in pairs(servers.data) do
		if v.id ~= currentJobId and v.maxPlayers > v.playing then
			game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, v.id, plr)
			return
		end
	end
	SendNotify("خطأ", "لم يتم العثور على سيرفر آخر", 5)
end)

