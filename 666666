end)

BackpackTarget_Button.MouseButton1Click:Connect(function()
	if TargetedPlayer and TargetedPlayer.Character then
		for i,v in pairs(TargetedPlayer.Backpack:GetChildren()) do
			v.Parent = plr.Backpack
		end
		for i,v in pairs(TargetedPlayer.Character:GetChildren()) do
			if v:IsA("Tool") then
				v.Parent = plr.Backpack
			end
		end
	end
end)

